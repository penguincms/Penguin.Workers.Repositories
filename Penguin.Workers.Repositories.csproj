<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageProjectUrl>https://github.com/penguincms/Penguin.Workers.Repositories</PackageProjectUrl>
    <Version>5.0.0</Version>
    <Description>IRepository implementation for tracking worker startup-shutdowns and other meta information not related to logging</Description>
    <AssemblyName>Penguin.Workers.Repositories</AssemblyName>
    <RootNamespace>Penguin.Workers.Repositories</RootNamespace>
    <DocumentationFile>Penguin.Workers.Repositories.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.4">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Penguin.Cms.Workers.Entities" Version="5.0.1-T9ZCSSJ" />
    <PackageReference Include="Penguin.Cms.Repositories" Version="5.0.1-T9ZCSSJ" />
  </ItemGroup>
  <Import Project="Penguin.Projects.Shared\Penguin.Projects.Shared.csproj" />
</Project>